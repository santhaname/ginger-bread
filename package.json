{
  "name": "ssr-react-v2",
  "version": "0.0.0",
  "type": "module",
  "nodemonConfig":{
    "exec": "npm run dev",
    "watch": ["server/*", "client/*"]
  },
  "scripts": {
    "start": "node ./server/bin/www",
    "watch": "node --inspect dist/server.js",
    "build": "webpack --config webpack.config.js",
    "dev": "npm-run-all build watch",
    "watch:dev": "nodemon"
  },
  "dependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "html-webpack-plugin": "^5.5.0",
    "react-dom": "^17.0.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
